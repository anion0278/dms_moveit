<launch>

  <!-- Params for 3D sensors config -->
  <rosparam command="load" file="$(find ur3_moveit)/config/sensors_3d.yaml" />

  <param name="octomap_resolution" type="double" value="0.02" />

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="ur3_workplace" />
  <include file="$(find ur3_moveit)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch.xml" />

</launch>
